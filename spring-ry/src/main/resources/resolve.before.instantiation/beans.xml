<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
       ">
    <bean id="beforeInstantiationUser" class="ry.resolveBeforeInstantiation.BeforeInstantiationUser" init-method="start">
        <property name="userId" value="1"/>
        <property name="userName" value="zhangsan"/>
        <property name="userAddress" value="beijing"/>
    </bean>
    <bean class="ry.resolveBeforeInstantiation.MyInstantiationAwareBeanPostProcessor"></bean>
</beans>